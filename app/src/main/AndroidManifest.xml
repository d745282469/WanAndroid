<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wanandroid">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".Activity.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activity.AboutUsActivity"></activity>
        <activity android:name=".Activity.MyCollectActivity" />
        <activity android:name=".Activity.SearchResultActivity" />
        <activity android:name=".Activity.SearchActivity" />
        <activity android:name=".Activity.KnowledgeTreeDetailActivity" />
        <activity android:name=".Activity.WebViewActivity" />
        <activity
            android:name=".Activity.RegActivity"
            android:theme="@style/AppTheme2"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".Activity.LoginActivity"
            android:theme="@style/AppTheme2"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>